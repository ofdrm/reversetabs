{
    "name":"ReverseTabs",
    "version":"1.0",
    "description":"Reverse ordering of selected/all tabs in current window",
    "manifest_version":3,
    "permissions":["clipboardWrite","tabs"],
    "action": {
        "default_icon": {                    
          "16": "images/icon16.png",          
          "32": "images/icon32.png"            
        },
        "default_title": "Reverse Tabs",
        "default_popup":"popup.html"
      },
      "icons": {
        "16": "images/icon16.png",          
        "32": "images/icon32.png"
      }
}